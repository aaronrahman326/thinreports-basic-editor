{
  "name": "ThinreportsEditor",
  "version": "0.0.0",
  "description": "Designer for Thinreports",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron app",
    "compile": "npm run compile:javascripts && npm run compile:styles && npm run compile:templates",
    "compile:javascripts": "node tasks/compile.javascripts.js",
    "compile:styles": "node tasks/compile.styles.js",
    "compile:templates": "node tasks/compile.templates.js",
    "package": "node tasks/build.js",
    "build": "cross-env NODE_ENV=production npm run compile && npm run package",
    "postinstall": "cd app && npm install"
  },
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "electron": "^1.6.10",
    "electron-packager": "^8.7.0",
    "electron-prebuilt": "^1.4.13",
    "js-yaml": "^3.9.1"
  }
}
